# Aura Singularity - Environment Configuration
# Copy this to .env.local

# OpenAI API Key (for Singularity Engine / RAG)
OPENAI_API_KEY=sk-proj-...

# Database Connection (Supabase / Postgres)
DATABASE_URL=postgresql://postgres:[PASSWORD]@db.supabase.co:5432/postgres

# Vercel / Next.js
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_ENABLE_GAUSSIAN_SPLATS=true
NEXT_PUBLIC_TFJS_BACKEND=webgpu

# Feature Flags
ENABLE_RAG_2025=true
ENABLE_QUANTUM_ENTANGLEMENT=true
